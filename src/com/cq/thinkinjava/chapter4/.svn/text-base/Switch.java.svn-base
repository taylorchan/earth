public class Switch {
    
    public static void main(String args[]) {
        char ch = 'a';
        switch (ch) {
            case 'a':
                System.out.println('a');
            case 'b':
                System.out.println('b');
            default:
                System.out.println("default c");
                
        }
        
    }
    
}
// a
// b
// default c
