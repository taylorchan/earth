enum Color {
    red, black, green;
    
}

class enumTest {
    
    public static void main(String args[]) {
        Color redcolor = Color.red;
        switch (redcolor) {
            case red:
                System.out.println("color is Red!!");
                break;
        }
        System.out.println("遍历枚举：");
        Color[] color = Color.values();
        for (Color color2 : color) {
            System.out.println(color2);
        }
    }
}
